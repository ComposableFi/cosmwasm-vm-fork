{
    "name": "cosmwasm-vm-ts",
    "version": "1.0.0",
    "description": "Composable CosmWasm VM TypeScript SDK",
    "source": [
        "src/index.ts",
        "src/typescript_bindings.js"
    ],
    "type": "module",
    "scripts": {
        "test": "node --experimental-specifier-resolution=node --experimental-fetch --loader ts-node/esm src/test.ts",
        "watch": "parcel watch",
        "build": "rm -rf dist && rm -rf .parcel-cache && parcel build --no-cache && cp src/index.d.ts dist/index.d.ts && cp src/typescript_bindings.d.ts dist/typescript_bindings.d.ts"
    },
    "main": [
        "dist/index.js",
        "dist/typescript_bindings.js"
    ],
    "types": [
        "src/typescript_bindings.d.ts",
        "src/common.ts"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "buffer": "^6.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.7.0",
        "@parcel/transformer-typescript-types": "^2.7.0",
        "@types/node": "^18.11.0",
        "parcel": "^2.7.0"
    }
}
