{
  "name": "@composable/cosmwasm-vm",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/ComposableFi/cosmwasm-vm.git"
  },
  "description": "Cosmwasm VM Typescript SDK",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./pkg/typescript_bindings.js": {
      "import": "./dist/lib/pkg/typescript_bindings.js",
      "require": "./dist/lib/pkg/typescript_bindings.js",
      "default": "./dist/lib/pkg/typescript_bindings.js"
    },
    "./lib/wasm/cw20_base.wasm": {
      "import": "./dist/wasm/cw20_base.wasm",
      "require": "./dist/wasm/cw20_base.wasm",
      "default": "./dist/wasm/cw20_base.wasm"
    },
    "./lib/wasm/reflect.wasm": {
      "import": "./dist/wasm/reflect.wasm",
      "require": "./dist/wasm/reflect.wasm",
      "default": "./dist/wasm/reflect.wasm"
    }
  },
  "type": "module",
  "scripts": {
    "run-file": "node --experimental-modules --experimental-wasm-modules --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm",
    "lint": "pnpm exec eslint --fix --ext .js,cjs,mjs,ts,mts .",
    "format": "pnpm exec prettier --write --ignore-path .gitignore .",
    "prebuild": "rm -rf dist",
    "build": "ttsc --project tsconfig.build.json",
    "release": "sh ./scripts/publish.sh"
  },
  "license": "ISC",
  "dependencies": {
    "wasmi": "^0.0.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@swc/helpers": "^0.4.12",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=18.12.1"
  }
}
